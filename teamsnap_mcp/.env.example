# TeamSnap MCP Server Configuration
# Copy this file to .env and fill in your values

# TeamSnap OAuth Access Token
# Get this from the parent directory's config.ini after running teamsnap_auth.py
TEAMSNAP_ACCESS_TOKEN=your_access_token_here

# Read-Only Mode (default: true)
# Set to "false" to enable write operations (create, update, delete)
# SECURITY: Keep this "true" unless you explicitly need write access
TEAMSNAP_READONLY=true
